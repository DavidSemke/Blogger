include tabList
include actionList


// recent and suggested options not shown if user not logged in
.content-sidebar
    if !user
        .site-overview
            .site-overview__name Tech Stack
            .site-overview__descrip A place for coders and tech enthusiasts
        .signup-prompt
            .signup-prompt__text Not a member?
            button.signup-prompt__button Sign Up
    else 
        +tabList('Recent', 'Suggested')
        +actionList(
            {
                'label': 'Blog 1',
                'img': '/images/profileIcon.png'
            },
            {
                'label': 'Blog 2',
                'img': '/images/profileIcon.png'
            }

        )