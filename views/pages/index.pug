extends ../layouts/browser
include ../components/card/blogCard

block page
  - const {blogs} = data

  .index-page
    - let i = 0
    while i < 2
      .index-page__blog-card-pair.blog-card-pair
        - let start = i*2
        each blog in blogs.slice(start, start+2)
          a(href=blog.url)
            +blogCard(blog)
      - i+=1